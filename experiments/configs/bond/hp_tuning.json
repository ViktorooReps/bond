{
    "output": "logs/distant/bond.log",
    "config_name": "bond-distant",
    "memory_needed": 12.0,
    "command": "python train.py",
    "args": [
        "--use_adaptive_scheduler",
        "--logging=100",
        "--adaptive_scheduler_patience=7",
        "--add_distant",
        "--framework=bond",
        "--resfile=res.final.txt",
        "--dataset=conll03",
        "--dataset_type=distant/train",
        "--model_type=roberta",
        "--model_name=roberta-base",
        "--gradient_accumulation_steps=1",
        "--max_seq_len=512",
        "--batch_size=8",
        "--head_dropout=0.3",
        "--bert_dropout=0.1",
        "--learning_rate=6e-05",
        "--lr_decrease=0.9",
        "--weight_decay=0.0",
        "--adam_epsilon=1e-06",
        "--adam_beta1=0.9",
        "--adam_beta2=0.999",
        "--no_entity_weight=0.7",
        "--warmup_proportion=0.2",
        "--self_training_lr_proportion=0.5",
        "--correct_frequency",
        "--use_kldiv_loss",
        "--label_keep_threshold=0.9",
        "--self_training_epochs=20",
        "--start_updates=1.0",
        "--end_updates=0.5",
        "--seed=42"
    ]
}