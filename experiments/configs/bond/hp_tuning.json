{
    "output": "logs/distant/bond.log",
    "config_name": "bond-distant",
    "memory_needed": 7.0,
    "command": "python train.py",
    "args": [
        "--logging=10000000",
        "--add_distant",
        "--framework=bond",
        "--resfile=res.final.txt",
        "--dataset=conll03",
        "--dataset_type=distant/train",
        "--model_type=roberta",
        "--model_name=roberta-base",
        "--gradient_accumulation_steps=1",
        "--max_seq_len=512",
        "--batch_size=4",
        "--head_dropout=0.3",
        "--bert_dropout=0.1",
        "--learning_rate=5e-05",
        "--weight_decay=1e-04",
        "--adam_epsilon=1e-06",
        "--adam_beta1=0.9",
        "--adam_beta2=0.999",
        "--no_entity_weight=0.7",
        "--warmup_proportion=1.0",
        "--ner_fit_epochs=0",
        "--use_linear_scheduler",
        ["--self_training_lr_proportion=0.3", "--self_training_lr_proportion=0.2", "--self_training_lr_proportion=0.1"],
        "--correct_frequency",
        "--use_kldiv_loss",
        "--label_keep_threshold=0.9",
        ["--self_training_epochs=2", "--self_training_epochs=3", "--self_training_epochs=5"],
        ["--start_updates=8.0", "--start_updates=4.0", "--start_updates=2.0"],
        ["--end_updates=2.0", "--end_updates=1.0", "--end_updates=0.5"],
        "--seed=42"
    ]
}